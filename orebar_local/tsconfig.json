{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2017",
        "sourceMap": true,
        "types": ["node"],
	"noImplicitAny": true,
	"preserveConstEnums": true,
	"noEmitOnError": true,
	"noUnusedParameters": true,
	"noUnusedLocals": true
    },
    "exclude": [
        "node_modules",
	"dist"
    ]
}
