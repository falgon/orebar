{
  "name": "oreblr",
  "productName": "oreblr",
  "version": "0.0.1",
  "description": "Simply Tumblr viewer on your menu/task bar.",
  "main": "./src/main.js",
  "scripts": {
    "build": "npm --prefix ../build/ i && npm i && ../build/node_modules/webpack/bin/webpack.js --config ../build/webpack.config.js && ./node_modules/tslint/bin/tslint ./src && ./node_modules/typescript/bin/tsc --p ./tsconfig.json && node ../build/build.js",
    "build:release": "npm --prefix ../build/ i && npm i && ../build/node_modules/webpack/bin/webpack.js --config ../build/webpack.production.config.js && ./node_modules/tslint/bin/tslint ./src && ./node_modules/typescript/bin/tsc --p ./tsconfig.json && node ../build/build.js",
    "asar": "../build/node_modules/asar/bin/asar.js pack ../oreblr ../build/Orebar.asar",
    "test": "../build/node_modules/webpack/bin/webpack.js --config ../build/webpack.config.js && ./node_modules/tslint/bin/tslint ./src && ./node_modules/typescript/bin/tsc --p ./tsconfig.json && ./node_modules/electron/cli.js ./src/main.js",
    "tsc": "../build/node_modules/webpack/bin/webpack.js --config ../build/webpack.config.js && ./node_modules/tslint/bin/tslint ./src &&  ./node_modules/typescript/bin/tsc --p ./tsconfig.json",
    "tscwatch": "./node_modules/typescript/bin/tsc --watch -p ./tsconfig.json",
    "webpackwatch": "../build/node_modules/webpack/bin/webpack.js --config ../build/webpack.config.js",
    "clean": "rm -f *.js *.map && find ./src -name \"*.js\" -print -exec rm {} \";\" && find ./src -name \"*.map\" -print -exec rm {} \";\"",
    "start": "../oreblr/node_modules/node/bin/node ../build/scripts/start.js"
  },
  "author": "roki <falgon53@yahoo.co.jp>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/falgon/oreblr.git"
  },
  "keywords": [
    "tumblr",
    "Electron",
    "menubar",
    "Orebar",
    "oreblr"
  ],
  "bugs": {
    "url": "https://github.com/falgon/oreblr/issues"
  },
  "devDependencies": {
    "@types/react-bootstrap": "^0.31.2",
    "asar": "^0.13.0",
    "awesome-typescript-loader": "^3.2.3",
    "css-loader": "^0.28.7",
    "react-bootstrap": "^0.31.3",
    "tslint": "^5.7.0"
  },
  "dependencies": {
    "@types/express": "4.0.37",
    "@types/jest": "^20.0.8",
    "@types/menubar": "5.1.5",
    "@types/node": "8.0.27",
    "@types/opener": "^1.4.0",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-icons": "^2.2.2",
    "@types/slideout": "0.1.19",
    "electron": "^1.7.6",
    "express": "4.15.4",
    "loaders.css": "^0.1.2",
    "menubar": "^5.2.3",
    "node": "^8.3.0",
    "node-sass": "^4.5.3",
    "oauth": "0.9.15",
    "opener": "^1.4.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-icons": "^2.2.5",
    "react-loaders": "^2.5.0",
    "react-scripts-ts": "2.6.0",
    "sass-loader": "^6.0.6",
    "slideout": "1.0.1",
    "source-map-loader": "^0.2.1",
    "tumblr": "0.4.1",
    "tumblr.js": "^1.1.1",
    "typescript": "2.5.2"
  }
}
